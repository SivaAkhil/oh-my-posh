{
    "final_space": true,
    "osc99": false,
    "console_title": false,
    "console_title_style": "",
    "console_title_template": "",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "horizontal_offset": 0,
            "vertical_offset": 0,
            "segments": [
                {
                    "type": "session",
                    "style": "diamond",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "foreground_templates": null,
                    "background": "#c386f1",
                    "background_templates": null,
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": null
                },
                {
                    "type": "path",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "foreground_templates": null,
                    "background": "#ff479c",
                    "background_templates": null,
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "enable_hyperlink": true,
                        "prefix": "  ",
                        "style": "folder"
                    }
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#193549",
                    "foreground_templates": null,
                    "background": "#fffb38",
                    "background_templates": null,
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "display_stash_count": true,
                        "display_upstream_icon": true
                    }
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "foreground_templates": null,
                    "background": "#6CA35E",
                    "background_templates": null,
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "  "
                    }
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\uE0B0",
                    "foreground": "#ffff66",
                    "background": "#906cff",
                    "properties": {
                        "prefix": " \uF81F "
                    }
                },
                {
                    "type": "shell",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#ffffff",
                    "foreground_templates": null,
                    "background": "#0077c2",
                    "background_templates": null,
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": " ﲵ "
                    }
                },
                {
                    "type": "os",
                    "style": "powerline",
                    "powerline_symbol":"\uE0B0",
                    "foreground": "#26C6DA",
                    "background": "#546E7A",
                    "properties": {
                      "postfix": " \uE0B1",
                      "macos": "mac",
                      "display_distro_name": "true",
                      "invert_powerline":true
                    }
                  },
                {
                    "type": "root",
                    "style": "powerline",
                    "powerline_symbol": "",
                    "invert_powerline": false,
                    "foreground": "#193549",
                    "foreground_templates": null,
                    "background": "#ffff66",
                    "background_templates": null,
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": null
                }
            ]
        },

        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
              {
                "type": "text",
                "style": "plain",
                "foreground": "lightRed",
                "properties": {
                  "prefix": "\n",
                  "text": "$",
                  "postfix": ""
                }
              }
            ]
          }
    ]
}
